{
  "compilerOptions": {
    // "types": ["wicg-file-system-access", "fs-browser"],
    "outDir": "dist",
    "declaration": true,
    "strictPropertyInitialization": false,
    "skipLibCheck": true,
    "baseUrl": "src",
    "paths": {
      "@db/*": ["db/*"],
      "@drag/*": ["drag/*"],
      "@fileSystem/*": ["fileSystem/*"],
      "@collections/*": ["models/collections/*"],
      "@directories/*": ["models/directories/*"],
      "@excludedDirectories/*": ["models/excludedDirectories/*"],
      "@files/*": ["models/files/*"],
      "@filesType/*": ["models/fileTypes/*"],
      "@rootDir/*": ["models/rootDirectories/*"],
      "@state/*": ["models/state/*"],
      "@settings/*": ["models/settings/*"],
      "@utils/*": ["utils/*"],
      "@models/*": ["models/*"]
    }
  },
  "include": ["./src/index.ts"],
  "extends": "../../tsconfig.json"
}
