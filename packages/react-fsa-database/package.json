{
  "name": "react-fsa-database",
  "version": "0.1.0",
  "description": "scan your local drive using the file system access api and store the results in a database",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "/dist"
  ],
  "scripts": {
    "clean": "rimraf dist/  && rimraf types/",
    "build": "rollup -c rollup.config.js && rimraf dist/types",
    "build:dev": "rollup -c rollup.dev.config.js",
    "build:watch": "rollup -c rollup.dev.config.js --watch",
    "lint": "eslint ./src --ext .ts,.tsx"
  },
  "author": "philstenning",
  "license": "ISC",
  "dependencies": {
    "@types/uuid": "^8.3.4",
    "dexie": "^3.2.1",
    "dexie-export-import": "^1.0.3",
    "dexie-react-hooks": "^1.1.1",
    "fsa-database": "workspace:*",
    "react": "^18.0.0",
    "react-dom": "^18.0.0",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^22.0.1",
    "@rollup/plugin-node-resolve": "^13.3.0",
    "@types/filesystem": "^0.0.32",
    "@types/react": "^18.0.8",
    "@types/react-dom": "^17.0.10",
    "@typescript-eslint/eslint-plugin": "^5.22.0",
    "@typescript-eslint/parser": "^5.22.0",
    "eslint": "^7.32.0 || ^8.2.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-prettier": "^8.5.0",
    "eslint-config-standard": "^17.0.0",
    "eslint-import-resolver-typescript": "^2.7.1",
    "eslint-plugin-import": "^2.25.3",
    "eslint-plugin-jsx-a11y": "^6.5.1",
    "eslint-plugin-n": "^15.0.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-promise": "^6.0.0",
    "eslint-plugin-react": "^7.28.0",
    "eslint-plugin-react-hooks": "^4.3.0",
    "prettier": "^2.6.2",
    "rimraf": "^3.0.2",
    "rollup": "^2.75.7",
    "rollup-plugin-dts": "^4.2.2",
    "rollup-plugin-terser": "^7.0.2",
    "typescript": "^4.6.3"
  }
}
