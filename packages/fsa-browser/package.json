{
  "name": "fsa-browser",
  "version": "0.1.0",
  "description": "",
  "main": "dist/cjs/index.js",
  "module": "dist/esm/index.js",
  "types": "dist/esm/index.d.ts",
  "files": [
    "/dist"
  ],
  "scripts": {
    "clean": "rimraf dist/  && rimraf types/",
    "build:X": "rollup -c && rimraf dist/**/types/",
    "build:dev": "rollup -c rollup.dev.config.js",
    "build:watch": "rollup -c rollup.dev.config.js --watch",
    "build:cjs": "tsc -p tsconfig.cjs.json",
    "build": "pnpm clean && concurrently pnpm:babel:*",
    "babel:esm": "babel --config-file ./babel.config.json src --extensions .ts --out-dir dist/esm --source-maps",
    "babel:cjs": "babel --config-file ./babel.cjs.config.json src --extensions .ts --out-dir dist/cjs --source-maps",
    "babel:tsc": "tsc"
  },
  "author": "philstenning",
  "license": "ISC",
  "dependencies": {
    "idb-keyval": "^6.1.0",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "@babel/cli": "^7.18.6",
    "@babel/core": "^7.18.6",
    "@babel/preset-env": "^7.18.6",
    "@babel/preset-typescript": "^7.18.6",
    "@rollup/plugin-commonjs": "^22.0.1",
    "@rollup/plugin-node-resolve": "^13.3.0",
    "@rollup/plugin-typescript": "^8.3.3",
    "@types/uuid": "^8.3.4",
    "@types/wicg-file-system-access": "^2020.9.5",
    "concurrently": "^7.2.2",
    "rimraf": "^3.0.2",
    "rollup": "^2.75.7",
    "rollup-plugin-copy": "^3.4.0",
    "rollup-plugin-dts": "^4.2.2",
    "rollup-plugin-filesize": "^9.1.2",
    "rollup-plugin-peer-deps-external": "^2.2.4",
    "rollup-plugin-terser": "^7.0.2",
    "tslib": "^2.4.0",
    "typescript": "^4.6.3"
  },"browserslist": "> 0.25%, not dead"
}
