{
  "$schema": "https://turborepo.org/schema.json",
  "baseBranch": "origin/main",
  "pipeline": {
    "build": {
      "dependsOn": ["^build"],
      "outputs": ["dist/**"]
    },
    "build:docs": {
      "dependsOn": ["^build"],
      "outputs": ["dist/**"]
    },
    "build:test": {
      "dependsOn": ["build"],
      "outputs": ["dist/**"]
    },
    "test": {
      "dependsOn": ["build:test"]
    },
    "clean": {},
    "build:examples": {
      "dependsOn": ["build"]
    }
  }
}
